{
    "name": "drupal-libraries/flysystem-stream-wrapper",
    "type": "library",
    "description": "A stream wrapper for Flysystem V2 & V3, without the StreamWrapper Manager.",
    "keywords": [
        "flysystem",
        "streamwrapper",
        "stream-wrapper"
    ],
    "homepage": "https://github.com/drupal-libraries/flysystem-stream-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Mandl",
            "email": "tech@m2m.at",
            "homepage": "https://cms.m2m.at",
            "role": "Developer"
        },
        {
            "name": "Lisa Ridley",
            "email": "lisa@codementality.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "league/flysystem": "^2.3|^3.0",
        "symfony/lock": "^5.3|^6.0|^7.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "ext-pcntl": "*",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.5|^9.6|^10.0",
    },
    "autoload": {
        "psr-4": {
            "DrupalLibraries\\FlysystemStreamWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DrupalLibraries\\FlysystemStreamWrapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test10": "vendor/bin/phpunit -c phpunit.10.xml",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit 1G"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
